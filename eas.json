{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "withoutCredentials": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true,
        "cache": {
          "key": "gradle-preview"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true,
        "image": "latest",
        "env": {
          "EXPO_PUBLIC_API_URL": "https://api.wallhaven.cc",
          "EXPO_OPTIMIZE": "true"
        },
        "cache": {
          "key": "split-apks"
        }
      }
    },
    "test": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true,
        "cache": {
          "key": "test-build-cache"
        },
        "env": {
          "EXPO_PUBLIC_API_URL": "https://api.wallhaven.cc"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
